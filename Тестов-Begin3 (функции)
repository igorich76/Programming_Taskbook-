uses PT4;

function Perimetr(side :double; side2: double) : double;
  begin
    Perimetr:= 2 * (side + side2);
  end;

function Square(side: double; side2: double) : double;
begin
  Square:= side * side2;
end;

var a, b : double;
begin
  Task('Begin3');
  
  read(a, b);  
  Write(Square(a,b));
  Write(Perimetr(a,b));
end.
